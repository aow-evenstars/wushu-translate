menu_71173_1=<font face="Default" color="#ffffff" >Vâng</font>
menu_71174_1=<font face="Default" color="#ffffff" >Được</font>
menu_71175_1=<font face="Default" color="#ffffff" >Được</font>
menu_71176_1=<font face="Default" color="#ffffff" >Ta đi ngay!</font>
menu_71177_1=<font face="Default" color="#ffffff" >Được</font>
menu_71178_1=<font face="Default" color="#ffffff" >Được</font>
menu_71179_1=<font face="Default" color="#ffffff" >Được</font>
menu_71180_1=<font face="Default" color="#ffffff" >Đa tạ tiền bối nhắc nhở, vãn bối bằng lòng thử một phen.</font>
