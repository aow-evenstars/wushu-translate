trigger_cbt_drwf_lzy_tsbd_true=<font color="#FF0000">Đã nhận được Bảo Tàng nơi này, thăm dò lần nữa không có phát hiện gì</font>
trigger_cbt_drwf_lzy_tsbd016_t=<font color="#00FF00">Dùng thanh gỗ cháy thắp sáng đèn cầy!</font>
